<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LikedActivity">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="112dp"
        android:paddingHorizontal="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL1"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img13" />

            <ImageView
                android:id="@+id/imgL2"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img12" />

            <ImageView
                android:id="@+id/imgL3"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img14" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL4"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img15" />

            <ImageView
                android:id="@+id/imgL5"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img19" />

            <ImageView
                android:id="@+id/imgL6"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img17" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL7"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img18" />

            <ImageView
                android:id="@+id/imgL8"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img16" />

            <ImageView
                android:id="@+id/imgL9"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img20" />
        </TableRow>

    </TableLayout>

    <androidx.cardview.widget.CardView
        android:layout_width="415dp"
        android:layout_height="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <HorizontalScrollView
            android:id="@+id/horizontalScrollView3"
            android:layout_width="414dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/blanco"
            android:paddingStart="90dp"
            tools:ignore="SpeakableTextPresentCheck">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/btnPerfil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="@color/blanco"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/ic_baseline_assignment_ind_24"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/btnLiked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="@color/blanco"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/ic_sharp_star_24"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/btnHome"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="@color/blanco"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/ic_baseline_home_24"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/btnSearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="@color/blanco"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/ic_baseline_search_24"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/btnSetting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:backgroundTint="@color/blanco"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/ic_baseline_settings_24"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />
            </LinearLayout>
        </HorizontalScrollView>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="My List"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>