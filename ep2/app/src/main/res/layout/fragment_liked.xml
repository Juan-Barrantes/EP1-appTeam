<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LikedActivity">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="112dp"
        android:paddingHorizontal="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL1"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img13" />

            <ImageView
                android:id="@+id/imgL2"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img12" />

            <ImageView
                android:id="@+id/imgL3"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img14" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL4"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img15" />

            <ImageView
                android:id="@+id/imgL5"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img19" />

            <ImageView
                android:id="@+id/imgL6"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img17" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgL7"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img18" />

            <ImageView
                android:id="@+id/imgL8"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img16" />

            <ImageView
                android:id="@+id/imgL9"
                android:layout_width="120dp"
                android:layout_height="90dp"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/img20" />
        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Mis favoritos"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>